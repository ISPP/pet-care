	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd 
	">

	<util:list id="messages">
		<value>/views/master-page/messages</value>
		<value>/views/misc/messages</value>
		<value>/views/security/messages</value>		
		<value>/views/welcome/messages</value>
		
		<value>/views/complaint/messages</value>
		<value>/views/comment/messages</value>
		<value>/views/search/messages</value>
		<value>/views/petOwner/messages</value>
		<value>/views/petSitter/messages</value>
		<value>/views/petShipper/messages</value>
		<value>/views/review/messages</value>
		<value>/views/booking/messages</value>
		<value>/views/supplier/messages</value>
		<value>/views/administrator/messages</value>
		<value>/views/pet/messages</value>
		<value>/views/trip/messages</value>
		<value>/views/registration/messages</value>
		<value>/views/company/messages</value>

	</util:list>

</beans>
